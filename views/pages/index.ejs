<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
  <head>
    <% include ../partials/head %>
  </head>
  <body class="slds">
    <header>
      <% include ../partials/siteHeader %>
    </header>
    <main>
      <% include ../partials/pageHeader %>
      <div class="slds-p-around--large">
        <div id="plaxHost">
          <div id="plax1">
          &nbsp;
          </div>
          <div id="plax2">
          &nbsp;
          </div>
          <div id="plax3">
          &nbsp;
          </div>
          <div id="plax4">
          &nbsp;
          </div>
          <div id="plax5">
          &nbsp;
          </div>
        </div>
      </div>
    </main>
    <!-- JavScript -->
    <script type="text/javascript" src="javascripts/min/parallax/entry.js"></script>
    <script type="text/javascript">
      var p = new Parallax.ParallaxContainer(window, 0.7);
      for (i = 1; i <= 5; ++i) {
          var star_layer = new Parallax.ParallaxSurface(document.querySelector('#plax' + i));
          p.addSurface(star_layer);
      }
    </script>
  </body>
</html>